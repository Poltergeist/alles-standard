{
  "name": "alles-standard",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@types/testing-library__jest-dom": "5.14.9",
    "gatsby": "5.13.7",
    "gatsby-plugin-pnpm": "1.2.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tsconfig-paths-webpack-plugin": "4.1.0"
  },
  "devDependencies": {
    "@babel/preset-react": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@chakra-ui/gatsby-plugin": "3.1.3",
    "@chakra-ui/react": "2.10.2",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.3.1",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "18.19.55",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "3.10.1",
    "@vitejs/plugin-react": "4.3.2",
    "babel-preset-gatsby": "3.13.2",
    "c8": "7.14.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-standard": "17.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.6.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "framer-motion": "11.11.8",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "22.1.0",
    "lint-staged": "6.1.1",
    "prettier": "2.8.8",
    "react-icons": "5.3.0",
    "typescript": "5.6.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "0.34.6"
  }
}
