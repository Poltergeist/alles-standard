{
  "name": "alles-standard",
  "description": "A TypeScript starter for Gatsby that includes all you need to build amazing projects",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (https://joaopedro.dev)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "gatsby": "5.13.7",
    "globals": "15.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsconfig-paths-webpack-plugin": "4.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@chakra-ui/gatsby-plugin": "3.1.3",
    "@chakra-ui/react": "2.8.2",
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "19.4.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/react-hooks": "8.0.1",
    "@types/node": "20.16.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "8.4.0",
    "@typescript-eslint/parser": "8.4.0",
    "@vitejs/plugin-react": "4.2.1",
    "babel-preset-gatsby": "3.9.0",
    "c8": "10.1.2",
    "eslint": "9.10.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "framer-motion": "11.3.29",
    "husky": "9.1.5",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "25.0.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "react-icons": "5.3.0",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5"
  }
}
