{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/static/*": [
        "./static/*"
      ]
    },
    "types": [
      "node",
      "@testing-library/jest-dom"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "gatsby-config.ts",
    "gatsby-node.ts",
    "gatsby-browser.ts",
    "gatsby-ssr.ts"
  ]
}
